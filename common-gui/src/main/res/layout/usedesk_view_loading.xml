<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/iv_loading_image"
        style="?attr/usedesk_common_view_loading_image" />

    <ProgressBar
        android:id="@+id/pb_loading"
        style="?attr/usedesk_common_progressbar"
        android:theme="?attr/usedesk_common_progressbar" />

    <TextView
        android:id="@+id/tv_loading_title"
        style="?attr/usedesk_common_view_loading_title" />

    <TextView
        android:id="@+id/tv_loading_text"
        style="?attr/usedesk_common_view_loading_text"
        tools:text="@string/usedesk_no_internet_text" />
</merge>